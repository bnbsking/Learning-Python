#data structure
#generator vs list

def g(n):
    i = 0
    while i < n:
        yield i     # append i in a Generator g(n) (irreversible data structure, O(1) space)
                    # next time calling g(n) execute from here
        i+=1

for i in g(3):      # generate an element while vist the element (fast)
                    # compare to list: generate all elements than visit all element(slow), O(n) space
    print(i)
    
p = g(3)
print(next(p))      # 0
print(next(p))      # 1
print(next(p))      # 2
#print(next(p))      # error

A = list(range(1,10))
B = [i for i in A]
C = (i for i in A)
A[0]=-100
print(A,B,C)
